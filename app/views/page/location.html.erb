
	<% @specimens.each do |specimen| %>
		<div style="padding-bottom: 10px; margin-bottom: 10px; border: 1px solid gray;" class="round">
			<table>
				<tr>
					<td style="width: 250px;"><a href="http://www.wikipedia.org/wiki/<%=specimen.species.split(" ")[0]%>"><%= specimen.species %></a></td>
					<td><img src="<%= specimen.thumbnail %>" ></img></td>
				</tr>
			</table>
		</div>
	<% end %>
	<br/>

	